//Generic using type T
class CompanyandModelnum1<T>
{
 T obj;
 CompanyandModelnum1(T obj) { 
 this.obj = obj; 
 } 
 public T getObject() {
 return this.obj; 
 }
}
//Generic using type T,U
//Two parameters
class CompanyandModelnum2<T, U>{
T Companyname; 
 U Modelnum; 
 public CompanyandModelnum2(T Companyname, U Modelnum) {
 this.Companyname = Companyname;
 this.Modelnum = Modelnum;
 }
 public T getCompanyname() {
 return Companyname;
 }
 public void setCompanyname(T Companyname) {
 this.Companyname = Companyname;
 }
 public U getModelnum() {
 return Modelnum;
 }
 public void setModelnum(U Modelnum) {
 this.Modelnum = Modelnum;
 }
 public String toString() {
 return "Another Laptop Company name and Model Number: [Company 
name=" + Companyname + ", Model Number=" + Modelnum + "]";
 }
}
