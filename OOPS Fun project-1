// Laptop is extended 
class Computer extends Laptop{
 Computer(String Nameoftheowner,int x,double y){ 
 super(Nameoftheowner,x,y);
 }
 Computer(String colour,String model){
 super(colour,model);
 }
 Computer(int x,int y){
 super(x,y);
 }
 Computer(String ColourofLaptop,int y){
 super(ColourofLaptop,y);
 }
 Computer(double x){
 super(x);
 }
 Computer(int x,int y,int z){
 super(x,y,z);
 }
 Computer(int x){
 super(x);
 }
 Computer(String Graphics,int x,String keyboard){
 super(Graphics,x,keyboard);
 }
 Computer(double x,String Countryoforigin){
 super(x,Countryoforigin);
 }
 //Method overriding
 //Changing Company Names
 void changeCompanyName(String laptop1,String laptop2){ 
 System.out.println("Laptop old name:"+laptop1);
 laptop1=laptop1+laptop2; 
 System.out.println("Laptop new name:"+laptop2);
 }
 //changing Model Name
 void changeModelName(String laptop1,String laptop2){ 
 System.out.println("laptop old Model name:"+laptop1);
 laptop1=laptop1+laptop2; 
 System.out.println("laptop new Model name:"+laptop2);
 }
}
