 void Laptop_check(int laptops_quantity) {
 if(laptops_quantity>=1) {
 System.out.println("\nStudent have Laptop");
 }
 else{
 System.out.println("\nStudent doesnt have Laptop");
 }
 }
 //Method overloading
 void laptopbox(int covers){
 if(covers>1){
 System.out.println("Laptop Box contains covers of laptop");
 }
 else{
 System.out.println("Laptop Box doesnt contain covers of laptop");
 }
 } 
 void laptopbox(int covers,double Chargerwatts){
 if(covers>1){
 System.out.println("Laptop Box contains covers of laptop");
 }
 else{
 System.out.println("Laptop Box doesnt contain covers of laptop");
 }
 if(Chargerwatts>45){
 System.out.println("Efficient Charger");
 }
 else{
 System.out.println("Not an Efficient Charger");
 }
 }
 void laptopbox(int covers,double Chargerwatts,Boolean LaptopGuide){
 if(covers>1){
 System.out.println("Laptop Box contains covers of laptop");
 }
 else{
 System.out.println("Laptop Box doesnt contain covers of laptop");
 }
 if(Chargerwatts>45){
 System.out.println("Efficient Charger");
 }
 else{
 System.out.println("Not an Efficient Charger");
 }
 if(LaptopGuide==true){
 System.out.println("Laptop box has guide");
 }
 else {
 System.out.println("Laptop box doesnt have guide");
 }
 }
 }
 //Abstract Class
 abstract class Mode{
 abstract void Turn_on();
 }
 class Aeroplane_Mode extends Mode{
 void Turn_on(){
 System.out.println("\nIt disables the Wi-Fi network card, Bluetooth, and 
any mobile data connection (3G/4G/5G) that you may have");
 }
 }
//Interface 
interface TypesofOperatingSystem{
 void print();
}
class Windows7 implements TypesofOperatingSystem{
 public void print()
 {
 System.out.println("My Laptop doesnt have Windows 7 operating 
System");
 }
}
class Windows8 implements TypesofOperatingSystem{
 public void print()
 {
 System.out.println("My Laptop doesnt have Windows 8 Operating 
System");
 }
}
class Windows10 implements TypesofOperatingSystem{
 public void print()
 {
 System.out.println("My Laptop doesnt have Windows 10 Operating 
System");
 }
}
class Windows11 implements TypesofOperatingSystem{
 public void print()
 {
 System.out.println("My Laptop has Windows 11 operating System");
 }
}
//Access specifiers
//Private access specifier
//It will not give output because private can be accessed only within the class
class GraphicCard{
private void show1() {
System.out.println("GTX1650");
}
//Default Access specifier
//It will give output because default can accessed within the package
void show2() {
System.out.println("Nvidia");
}
public static void main(String args[]) {
GraphicCard object = new GraphicCard();
object.show1();
}
}
//Protected Access Specifier
//The protected access modifier is accessible within package and outside the 
package but through inheritance only.
class Processor{
protected void show3() {
System.out.println("processor");
}
}
//public Access Specifier
//The public access modifier is accessible everywhere.
class Webcam{
public void show4(){
System.out.println("Laptop has Webcam");
}
} 
//Final class
final class Memory {
 public int data = 30;
 public void show5() {
 System.out.println(""+data);
 }
}
//Exceptional Handling
//Try..Catch Block
class Gaming{
void WorkStation(){
try {
int Max_Battery_hours[]= {6,9,17,18,20};
System.out.println(Max_Battery_hours[5]);
}catch(Exception e) {
System.out.println(e);
}
}
}
//Multiple Catch Block
class Clockspeed{
double Baseclock[]= {3.3,2.1,1.9,2.2,2.1,2.6};
 int clock1 = 3;
 int clock2 = 0;
 void Base_Clock(){
 try{
 System.out.println(clock1/clock2);
 System.out.println("6th element of given array = " + Baseclock[6]);
 }catch(ArithmeticException e){ 
 System.out.println(e);
 }
 catch(ArrayIndexOutOfBoundsException e){
 System.out.println(e);
 }
 catch(Exception e) {
 System.out.println(e);
 }
 System.out.println("Remaining code after exception handling.");
 }
}
//Null point Exception
class Coolingpad
{
void Check_Coolingpad() {
 try {
 String Cooling_pad = null; 
 System.out.println(Cooling_pad.charAt(0));
 } catch(NullPointerException e) {
 System.out.println("NullPointerException..");
 }
 }
}
//Nested Try 
class Cache_Memory { 
void cache_data(){
try{ 
 try{ 
 System.out.println("Divide by 0"); 
 int x =20/0; 
 }
catch(ArithmeticException e) 
{ 
 System.out.println(e); 
} 
try{ 
int cache[]= {24,14,12,18,10,36}; 
 cache[25]=40; 
} 
catch(ArrayIndexOutOfBoundsException e) 
 { 
System.out.println(e); 
 } 
 System.out.println("Other statement"); 
 } 
catch(Exception e) 
{ 
System.out.println("handled the exception (outer catch)"); 
 } 
 System.out.println("normal flow.."); 
 } 
}
//Finally Clause
class Turbo{
 void turbo_GHz() {
 double GHz[] = {4.5,4.4,4.2,4.1,4.0};
 try {
 System.out.println("Access element:" + GHz[7]);
 } catch (ArrayIndexOutOfBoundsException e) {
 System.out.println("Exception thrown :" + e);
 }finally {
 System.out.println("First element value: " + GHz[0]);
 System.out.println("The finally statement is executed");
 }
 }
}
//Throw Clause
class BatteryLife { 
void check_BatteryLife(){ 
 try { 
 int min_Battery_hours = 4; 
 if (min_Battery_hours< 5) throw new ArithmeticException();
 }catch (Exception e){ 
 System.out.println(e);
 }
 }
}
//Overflow
//Underflow
class Overflow extends Exception{
Overflow (){
 super("Overflow Exception (input >=2147483647)");
 }
}
class Underflow extends Exception{
 Underflow (){
 super("Underflow Exception(input <=2147483647)");
 }
}
class flows{
void areaofLaptopKeyboard() {
Scanner sc=new Scanner(System.in);
try {
System.out.println("Enter the area of the laptop keyboard:");
int area= sc.nextInt();
if(area>=Integer. MAX_VALUE) 
throw new Overflow();
else if(area<=Integer. MIN_VALUE) 
throw new Underflow();
else
 System.out.println("area of the laptop:"+area);
}
catch(Underflow e) {
 System.out.println(e);
}
 catch(Overflow e) {
 System.out.println(e);
 }
}
void TotalCost() {
Scanner sc=new Scanner(System.in);
try {
System.out.println("\nEnter the Total cost of the laptop:");
int totalcost= sc.nextInt();
if(totalcost>=Integer. MAX_VALUE) 
throw new Overflow();
else if(totalcost<=Integer. MIN_VALUE) 
throw new Underflow();
else
 System.out.println("Total cost of the laptop:"+totalcost);
}
catch(Underflow e) {
 System.out.println(e);
}
 catch(Overflow e) {
 System.out.println(e);
 }
}
void Profit() {
Scanner sc=new Scanner(System.in);
try {
System.out.println("Enter the profit to the seller:");
int profit= sc.nextInt();
if(profit>=Integer. MAX_VALUE) 
throw new Overflow();
else if(profit<=Integer. MIN_VALUE) 
throw new Underflow();
else
 System.out.println("Profit to the seller:"+profit);
}
catch(Underflow e) {
 System.out.println(e);
}
 catch(Overflow e) {
 System.out.println(e);
 }
}
void LengthofDisplay() {
Scanner sc=new Scanner(System.in);
try {
System.out.println("Enter the length of display of the laptop:");
int length= sc.nextInt();
if(length>=Integer. MAX_VALUE) 
throw new Overflow();
else if(length<=Integer. MIN_VALUE) 
throw new Underflow();
else
 System.out.println("length of display of the laptop:"+length);
}
catch(Underflow e) {
 System.out.println(e);
}
 catch(Overflow e) {
 System.out.println(e);
 }
 }
}
//Thread class
//create a new thread
class NewThread extends Thread{
public static Object obj = new Object(); 
String[] temp=new String[10];
NewThread(String[] temp){
for(int i=0;i<3;i++)
this.temp[i]=temp[i];
}
//run
public void run() {
try {
for(int i=0;i<3;i++) {
System.out.print(temp[i]+" ");
//sleep
Thread.sleep(1000);
//Sychronized
synchronized (obj) 
 { 
//wait
 obj.wait(2000); 
 System.out.println("-----> Next Object is in waiting 
state and woken after 2 seconds"); 
 } 
 } 
}catch(Exception e){
System.out.println(e);
}
}
}
//create a new thread
class LaptopThread extends Thread{
public static Object obj1 =new Object();
String Specifications[] = new String[100] ;
 LaptopThread(String []Specifications ) {
 for(int i=0;i<4;i++) 
 this.Specifications[i]=Specifications[i];
 }
 //run
 public void run() {
 try {
 for(int i=0;i<4;i++) {
 System.out.print(Specifications[i]+" ");
 //sleep
 Thread.sleep(1000);
 //sychronized
 synchronized(obj1)
 {
 //wait
 obj1.wait(3000);
 System.out.println("-----> Next Object is 
in waiting state and woken after 3 seconds"); 
 }
 }
 }catch(Exception e){
 System.out.println(e);
 }
 }
}
class Bootprocess extends Thread 
{ 
 public void run() 
 { 
 synchronized(this) 
 { 
 System.out.println("Starting of the " + 
Thread.currentThread().getName()); 
 try { 
 this.wait(); 
 } 
 catch (InterruptedException e) { 
 System.out.println(e);
 } 
 System.out.println(Thread.currentThread().getName() + "...its notified"); 
 } 
 } 
} 
class Power_button extends Thread { 
Bootprocess bootprocess; 
Power_button(Bootprocess bootprocess1) 
 { 
 this.bootprocess = bootprocess1; 
 } 
 public void run() 
 { 
 synchronized(this.bootprocess) 
 { 
 System.out.println("Starting of " + Thread.currentThread().getName()); 
 try { 
 this.bootprocess.wait(); 
 } 
 catch (InterruptedException e) { 
 System.out.println(e); 
 } 
 System.out.println(Thread.currentThread().getName() + "...its notified"); 
 } 
 } 
} 
class Screen_appears extends Thread 
{ 
Bootprocess bootprocess; 
Screen_appears(Bootprocess bootprocess) 
 { 
 this.bootprocess = bootprocess; 
 } 
 public void run() 
 { 
 synchronized(this.bootprocess) 
 { 
 System.out.println("Starting of " + Thread.currentThread().getName()); 
 //notify() method 
 this.bootprocess.notify(); 
 System.out.println(Thread.currentThread().getName() + "...its notified"); 
 } 
 } 
}
class Send extends Thread{
 public void run() 
 { 
 synchronized(this) 
 { 
 System.out.println("Sending.."+ Thread.currentThread().getName()); 
 try { 
 this.wait(); 
 } 
 catch (InterruptedException e) { 
 System.out.println(e);
 } 
 System.out.println(Thread.currentThread().getName() + "Sent"); 
 } 
 } 
} 
class Receive extends Thread { 
Send send; 
Receive(Send send) 
 { 
 this.send = send; 
 } 
 public void run() 
 { 
 synchronized(this.send) 
 { 
 System.out.println("Receiving " + Thread.currentThread().getName()); 
 this.send.notifyAll(); 
 System.out.println(Thread.currentThread().getName() + "Received"); 
 } 
 } 
} 
//Dead Lock
class Dead_lock
{
 static void sleep(long millis)
 {
 try
 {
 Thread.sleep(millis);
 }
 catch (InterruptedException e)
 {
 System.out.println(e);
 }
 }
public void test1(Dead_lock s2) {
}
}
class Dead_lock1
{
 synchronized void test1(Dead_lock1 s2)
 {
 System.out.println("Thread 1 locked");
 Dead_lock.sleep(1000);
 s2.test2();
 System.out.println("Thread 1 locked");
 }
 synchronized void test2()
 {
 System.out.println("Thread 2 locked");
 Dead_lock.sleep(1000);
 System.out.println("Thread2 locked");
 }
}
class Thread1 extends Thread
{
 private Dead_lock1 s1;
 private Dead_lock1 s2;
 public Thread1(Dead_lock1 s1, Dead_lock1 s2)
 {
 this.s1 = s1;
 this.s2 = s2;
 }
 @Override
 public void run()
 {
 s1.test1(s2);
 }
}
class Thread2 extends Thread
{
 private Dead_lock1 s1;
 private Dead_lock1 s2;
 public Thread2(Dead_lock1 s1, Dead_lock1 s2)
 {
 this.s1 = s1;
 this.s2 = s2;
 }
 public void run()
 {
 s2.test1(s1);
 }
}
//Race condition
class RaceCondition implements Runnable{
 private int n = 0;
 public void increment() {
 try {
 Thread.sleep(10);
 } catch (InterruptedException e) {
 System.out.println(e);
 }
 n++;
 }
 public void decrement() { 
 n--;
 }
 public int getValue() {
 return n;
 }
 
 @Override
 public void run() {
 this.increment();
 System.out.println(" After incrementing, the Value of Thread:"+ 
Thread.currentThread().getName() + " " + this.getValue());
 this.decrement();
 System.out.println("At last Value of Thread: " + 
Thread.currentThread().getName() + " " + this.getValue()); 
 }
}
//final keyword
// final variable
class Basespeed{ 
final double base_speed= 2.59;//final variable 
void run(){ 
 double base_speed= 3.00; 
 System.out.println("base speed is"+base_speed);
}
}
