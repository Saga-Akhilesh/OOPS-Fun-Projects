 void laptopnamelength(String laptop1){ 
 int length=0;
 char[] strCharArray=laptop1.toCharArray();
 for(char c:strCharArray)
 {
 length++;
 }
 System.out.println("Laptop Name Length="+length);
 }
 void laptopnameRev(String laptop1){ 
 int length=0;
 char[] strCharArray=laptop1.toCharArray();
 for(char c:strCharArray)
 {
 length++;
 }
 System.out.print("\nlaptop name in Reverse:");
 while(length>0)
 {
 System.out.print(strCharArray[length-1]);
 length--;
 }
 }
